@model IEnumerable<WebAppPersonalEjemplo.Models.PersonalEnt>

@{
    ViewBag.Title = "Listado";
    TempData["listado"] = Model; //se envia el listado en TempData
}

<h2>Listado</h2>

<p>
    @Html.ActionLink("Nuevo Personal", "Nuevo")
    
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PersonalId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nombres)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Apellidos)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Correo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PersonalId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nombres)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Apellidos)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Correo)
        </td>
      
    </tr>
}

</table>
